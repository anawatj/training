<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.tao.training.domain.master.CourseType" table="MAS_M_COURSE_TYPE">
		<id name="id" column="ID" type="long">
			<generator class="sequence">
				<param name="sequence">MAS_M_COURSE_TYPE_SEQ</param>
			</generator>
		</id>
		<property name="code" column="COURSE_TYPE_CODE" length="20"></property>
		<property name="description" column="COURSE_TYPE_DESC" length="200"></property>

		<property name="createdBy" column="CREATED_BY" length="20"></property>
		<property name="createdDate" column="CREATED_DATE"></property>
		<property name="updatedBy" column="UPDATED_BY" length="20"></property>
		<property name="updatedDate" column="UPDATED_DATE"></property>
	</class>
</hibernate-mapping>