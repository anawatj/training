<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>

	<class name="com.tao.training.domain.master.DocStatus" table="MAS_M_DOC_STATUS">
		<id name="id" column="ID" type="long">
			<generator class="sequence">
				<param name="sequence">MAS_M_DOC_STATUS</param>
			</generator>
		</id>
		<property name="code" column="DOC_STATUS_CODE" length="20"></property>
		<property name="description" column="DOC_STATUS_DESC" length="200"></property>

		<property name="createdBy" column="CREATED_BY" length="20"></property>
		<property name="createdDate" column="CREATED_DATE"></property>
		<property name="updatedBy" column="UPDATED_BY" length="20"></property>
		<property name="updatedDate" column="UPDATED_DATE"></property>
	</class>
</hibernate-mapping>